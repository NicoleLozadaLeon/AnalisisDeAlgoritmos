void Main()
{
	int n = 10;
	int result = fibonacci(n);
	Console.Write(result);
}

int fibonacci(int n){
	int presente = 0;
	int pasado = 0;
	int futuro = 0;
	for (int i = 1;i <=n; ++i){
		if ( i == 1 ){
			futuro = 1; // 0
			
		} else if (i== 2){
			presente = futuro; //1
			futuro = pasado + presente;
			
		} else{
			pasado = presente; //2
			presente = futuro; //3
			futuro = pasado + presente; //5
			
		}
	}
	return presente;
}
